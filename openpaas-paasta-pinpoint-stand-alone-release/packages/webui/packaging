# abort script on any command that exits with a non zero value
set -e

archive="webui/pinpoint-web-1.6.0-SNAPSHOT.war"

if [[ -f $archive ]] ; then
  echo "Archive found"
  echo "Copying webui war..."
  cp webui/pinpoint-web-1.6.0-SNAPSHOT.war ${BOSH_INSTALL_TARGET}
else
  echo "Archive not found"
  exit 1
fi
