# abort script on any command that exits with a non zero value
set -e

archive="collector/pinpoint-collector-1.6.0-SNAPSHOT.war"

if [[ -f $archive ]] ; then
  echo "Archive found"
  echo "Copying collector war..."
  cp collector/pinpoint-collector-1.6.0-SNAPSHOT.war ${BOSH_INSTALL_TARGET}
else
  echo "Archive not found"
  exit 1
fi
